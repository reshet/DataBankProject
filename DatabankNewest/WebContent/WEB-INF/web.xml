<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.mresearch.databank.server.GreetingServiceImpl</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>userAccountService</servlet-name>
    <servlet-class>com.mresearch.databank.server.UserAccountServiceImpl</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>userResearchService</servlet-name>
    <servlet-class>com.mresearch.databank.server.UserSocioResearchServiceImpl</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>adminResearchService</servlet-name>
    <servlet-class>com.mresearch.databank.server.AdminSocioResearchServiceImpl</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>rxStorageServe</servlet-name>
    <servlet-class>com.mresearch.databank.server.RxStorageServe</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>uploadServlet</servlet-name>
    <servlet-class>com.mresearch.databank.server.RxUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/databanknewest/greet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>userAccountService</servlet-name>
    <url-pattern>/databanknewest/userAccountService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>userResearchService</servlet-name>
    <url-pattern>/databanknewest/userResearchService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>adminResearchService</servlet-name>
    <url-pattern>/databanknewest/adminResearchService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>rxStorageServe</servlet-name>
    <url-pattern>/databanknewest/serve</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>uploadServlet</servlet-name>
    <url-pattern>*.gupld</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>DatabankNewest.html</welcome-file>
  </welcome-file-list>
</web-app>