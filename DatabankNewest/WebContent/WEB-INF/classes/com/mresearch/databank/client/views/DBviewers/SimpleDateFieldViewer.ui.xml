	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:third="urn:import:org.zenika.widget.client.datePicker">
 <ui:with field='res' type='com.mresearch.databank.Resources.CoolStylesBundle'/>
	<g:HTMLPanel>
		<table>
			<tr>
				<td>
					<g:Label ui:field="field_name" styleName="{res.coolStyles.category}"></g:Label>
