<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			border: 1 1 1 1;
		}
		.category
		{
			color: #772222;
			font-weight: bold;
			font: 16px;
			vertical-align: text-top;
			font: NeoSans;
		}
	</ui:style>
	<g:HTMLPanel styleName="{.important}">
		<table>
			<tr>
				<td>
					<g:Label ui:field="entity_name" styleName="{.category}"></g:Label>
				</td>
				<td>
					<g:Label ui:field="items_list"></g:Label>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 