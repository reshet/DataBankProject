<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.category
		{
			font-weight: bold;
			font: 15px;
			vertical-align: text-top;
		}
		.tableResearch
		{
			font-weight: normal;
			padding: 2px;
			margin-left: 15px;
		}
	</ui:style>
	<g:HTMLPanel>
		<div class="gray25 bold">
		<g:Label ui:field="field_name"></g:Label>
		</div>
		<g:FlexTable ui:field="subunits_table" styleName="{style.tableResearch}"></g:FlexTable>
	</g:HTMLPanel>
</ui:UiBinder> 