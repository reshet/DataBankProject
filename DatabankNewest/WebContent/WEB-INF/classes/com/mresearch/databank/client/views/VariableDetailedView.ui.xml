<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:views="urn:import:com.mresearch.databank.client.views">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<h2>Просмотр переменной</h2>
		<g:HTMLPanel ui:field="main_html">
			<table>
				<tr>
					<td>Имя переменной: </td><td><g:Label ui:field="varCode"></g:Label></td>
				</tr>
				<tr>
					<td>Текст вопроса: </td><td><g:Label ui:field="varText"></g:Label></td>
				</tr>
				<tr>
					<td>Code Scheme: </td><td><g:FlexTable ui:field="codeSchemeTbl"></g:FlexTable></td>
				</tr>
				<tr>
					<td>Переменная использовалась:</td><td><g:FlexTable ui:field="generalizedTbl"></g:FlexTable></td>
				</tr>
			</table>
		</g:HTMLPanel>
		<g:HorizontalPanel ui:field="save_pnl"></g:HorizontalPanel>
		<g:VerticalPanel ui:field="graph_pnl" width="800px" height="700px"></g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 