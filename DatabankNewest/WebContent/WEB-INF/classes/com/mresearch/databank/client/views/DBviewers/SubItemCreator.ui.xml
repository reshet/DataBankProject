<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:third="urn:import:org.zenika.widget.client.datePicker">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:ScrollPanel>
		<g:VerticalPanel>
			<g:HorizontalPanel>
				<g:Label>Имя: </g:Label>
				<g:TextBox ui:field="name_field"></g:TextBox>
			</g:HorizontalPanel>
			
			<g:VerticalPanel ui:field="host">
			</g:VerticalPanel>
			<g:HorizontalPanel>
				<g:Button ui:field="doAdd">ОК</g:Button>
				<g:Button ui:field="doCancel">Отмена</g:Button>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:ScrollPanel>	
</ui:UiBinder> 