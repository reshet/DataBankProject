<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
		<g:HTMLPanel>
			<h3>
				Добавить организацию 
			</h3>
			<table>
				<tr>
					<td>Название организации: </td><td><g:TextBox ui:field="orgName" width="400px"></g:TextBox></td>
				</tr>
				<tr>
					<td>Адрес: </td><td><g:TextBox ui:field="orgAdress" width="400px"></g:TextBox></td>
				</tr>
				<tr>
					<td>Контактный телефон: </td><td><g:TextBox ui:field="orgTel" width="400px"></g:TextBox></td>
				</tr>
				<tr>
					<td><g:Button ui:field="cancelBtn">Отмена</g:Button></td><td><g:Button ui:field="submitBtn">Добавить!</g:Button></td>
				</tr>
			</table>
		</g:HTMLPanel>
</ui:UiBinder> 