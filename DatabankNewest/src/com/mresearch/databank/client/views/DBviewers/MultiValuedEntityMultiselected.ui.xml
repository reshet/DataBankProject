<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			border: 1 1 1 1;
		}
	</ui:style>
	<g:HTMLPanel styleName="{.important}">
		<table>
			<tr>
				<td>
					<g:Label ui:field="entity_name"></g:Label>
				</td>
				<td>
					<g:TextBox ui:field="items_list"></g:TextBox>
				</td>
			</tr>
			<tr>
				<td>
					<g:Anchor ui:field="edit_selection">изменить выбор</g:Anchor>
				</td>
				<td>
					<g:Anchor ui:field="edit_field">редактировать поле</g:Anchor>
				</td>
				<td>
					<g:Anchor ui:field="edit_items">к элементам</g:Anchor>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 