<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
				.category
		{
			color: #773344;
			font-weight: bold;
			font: 14px;
			vertical-align: text-top;
			font: NeoSans;
		}
	</ui:style>
	<g:HTMLPanel>
		<table>
			<tr>
				<td>
					<g:Label ui:field="field_name" styleName="{.category}"></g:Label>
				</td>
				<td>
					<g:VerticalPanel ui:field="download_panel"></g:VerticalPanel>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 